{
  "coverageAnalysis": "perTest",
  "inPlace": false,
  "mutate": ["src/**/*.js"],
  "commandRunner": {
    "command": "npm run test:unit"
  },
  "timeoutMS": 10000,
  "reporters": ["dashboard", "clear-text", "html", "progress"],
  "htmlReporter": {
    "baseDir": "_reports/mutation"
  },
  "thresholds": {
    "high": 100,
    "low": 100,
    "break": 100
  },
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": false
}
