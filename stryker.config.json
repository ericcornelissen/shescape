{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "coverageAnalysis": "perTest",
  "inPlace": false,
  "mutate": ["src/**/*.js"],
  "reporters": ["dashboard", "clear-text", "html", "progress"],
  "htmlReporter": {
    "baseDir": "_reports/mutation"
  },
  "thresholds": {
    "high": 100,
    "low": 100,
    "break": 100
  },
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": false
}
